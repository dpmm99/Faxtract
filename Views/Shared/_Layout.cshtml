<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Faxtract</title>
</head>
<body>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div>
            This work is dedicated to the public domain.
        </div>
    </footer>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>